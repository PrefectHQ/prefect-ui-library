{
    "name": "@prefecthq/orion-design",
    "version": "1.0.0-beta.0",
    "private": false,
    "keywords": [
        "prefect 2.0",
        "vue3"
    ],
    "scripts": {
        "build:demo": "vite build --mode=demo",
        "build": "vue-tsc && tsc-alias && vite build",
        "dev": "vue-tsc -w & tsc-alias -w & vite build -w",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "serve": "vite --mode=demo"
    },
    "main": "./dist/orion-design.umd.js",
    "module": "./dist/orion-design.es.js",
    "exports": {
        ".": {
            "import": "./dist/orion-design.es.js",
            "require": "./dist/orion-design.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "types": "./dist/types/index.d.ts",
    "devDependencies": {
        "@prefecthq/eslint-config": "1.0.15",
        "@prefecthq/vue-compositions": "0.1.21",
        "@types/node": "^17.0.31",
        "@types/tailwindcss": "3.0.10",
        "@vitejs/plugin-vue": "2.3.2",
        "autoprefixer": "10.4.7",
        "date-fns": "2.28.0",
        "date-fns-tz": "1.3.4",
        "eslint": "8.14.0",
        "postcss": "8.4.13",
        "tailwindcss": "3.0.24",
        "tsc-alias": "1.6.7",
        "typescript": "^4.6.4",
        "vite": "2.9.7",
        "vue-tsc": "0.34.11"
    },
    "peerDependencies": {
        "pinia": "2.0.11",
        "vue": "3.2.33",
        "vue-router": "4.0.12"
    },
    "dependencies": {
        "@prefecthq/prefect-design": "0.0.1",
        "axios": "0.27.2"
    }
}
